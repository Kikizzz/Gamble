<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="studio.bowman.gamblegame.ReactionChoose"
    android:background="@drawable/background">

    <include
        layout="@layout/topbar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="0dp"
        android:id="@+id/include3" />

    <include
        layout="@layout/navigation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="0dp"
        android:id="@+id/include4" />

    <Space
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/include3"
        app:layout_constraintHorizontal_bias="0.504"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/include4"
        android:id="@+id/space9"
        app:layout_constraintVertical_bias="0.49" />

    <ImageButton
        android:id="@+id/choice3"
        android:background="@null"
        android:layout_width="125dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/buy_keys"
        app:layout_constraintRight_toLeftOf="@+id/space9"
        android:layout_marginRight="4dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toTopOf="@+id/space9" />

    <ImageButton
        android:id="@+id/choice4"
        android:background="@null"
        android:layout_width="125dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/buy_keys"
        app:layout_constraintLeft_toRightOf="@+id/space9"
        android:layout_marginLeft="4dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toTopOf="@+id/space9" />

    <ImageButton
        android:id="@+id/choice1"
        android:background="@null"
        android:layout_width="125dp"
        android:layout_height="80dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toTopOf="@+id/choice3"
        app:layout_constraintLeft_toLeftOf="@+id/choice3"
        app:layout_constraintRight_toRightOf="@+id/choice3"
        app:srcCompat="@drawable/buy_keys" />

    <ImageButton
        android:id="@+id/choice2"
        android:background="@null"
        android:layout_width="125dp"
        android:layout_height="80dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toTopOf="@+id/choice4"
        app:layout_constraintLeft_toLeftOf="@+id/choice4"
        app:layout_constraintRight_toRightOf="@+id/choice4"
        app:srcCompat="@drawable/buy_keys" />

    <ImageButton
        android:id="@+id/choice6"
        android:background="@null"
        android:layout_width="125dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/buy_keys"
        app:layout_constraintLeft_toRightOf="@+id/space9"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@+id/space9" />

    <ImageButton
        android:id="@+id/choice5"
        android:background="@null"
        android:layout_width="125dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/buy_keys"
        app:layout_constraintRight_toLeftOf="@+id/space9"
        android:layout_marginRight="4dp"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@+id/space9" />

    <ImageButton
        android:id="@+id/choice8"
        android:background="@null"
        android:layout_width="125dp"
        android:layout_height="80dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/choice6"
        app:layout_constraintRight_toRightOf="@+id/choice6"
        app:layout_constraintTop_toBottomOf="@+id/choice6"
        app:srcCompat="@drawable/buy_keys" />

    <ImageButton
        android:id="@+id/choice7"
        android:background="@null"
        android:layout_width="125dp"
        android:layout_height="80dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/choice5"
        app:layout_constraintRight_toRightOf="@+id/choice5"
        app:layout_constraintTop_toBottomOf="@+id/choice5"
        app:srcCompat="@drawable/buy_keys" />
</android.support.constraint.ConstraintLayout>
