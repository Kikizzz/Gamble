<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="studio.bowman.gamblegame.Crash"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp"
    android:background = "@drawable/background">

    <studio.bowman.gamblegame.CrashView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        android:id="@+id/crashView"
        app:layout_constraintTop_toBottomOf="@+id/v1" />

    <TextView
        android:id="@+id/valuedisplay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="value"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/crashView" />

    <ImageButton
        android:id="@+id/play"
        android:layout_width="89dp"
        android:layout_height="59dp"
        android:layout_marginRight="32dp"
        android:layout_marginTop="16dp"
        app:layout_constraintRight_toLeftOf="@+id/space3"
        app:layout_constraintTop_toBottomOf="@+id/valuedisplay"
        app:srcCompat="@drawable/bail" />

    <ImageButton
        android:id="@+id/drop"
        android:layout_width="89dp"
        android:layout_height="59dp"
        android:layout_marginLeft="31dp"
        android:layout_marginTop="16dp"
        app:layout_constraintLeft_toRightOf="@+id/space3"
        app:layout_constraintTop_toBottomOf="@+id/valuedisplay"
        app:srcCompat="@drawable/bail" />

    <Space
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/valuedisplay"
        android:id="@+id/space3"
        app:layout_constraintHorizontal_bias="0.501" />

    <TextView
        android:id="@+id/display"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="16dp"
        android:text="display"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/drop" />

    <TextView
        android:id="@+id/balance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="balance"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/mainBtn" />

    <TextView
        android:id="@+id/profit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="16dp"
        android:text="profit"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/display" />

    <TextView
        android:id="@+id/debug"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="debug"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toTopOf="@+id/betdisplay" />

    <TextView
        android:id="@+id/betdisplay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="bet"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toTopOf="@+id/balance" />

    <ImageButton
        android:id="@+id/plusmin"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/betdisplay"
        app:layout_constraintLeft_toRightOf="@+id/betdisplay"
        app:layout_constraintTop_toTopOf="@+id/betdisplay"
        app:layout_constraintVertical_bias="0.489"
        app:srcCompat="@drawable/plus" />

    <ImageButton
        android:id="@+id/plusmid"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/plusmin"
        app:layout_constraintLeft_toRightOf="@+id/plusmin"
        app:layout_constraintTop_toTopOf="@+id/plusmin"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/plus" />

    <ImageButton
        android:id="@+id/plusmax"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/plusmid"
        app:layout_constraintLeft_toRightOf="@+id/plusmid"
        app:layout_constraintTop_toTopOf="@+id/plusmid"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/plus" />

    <ImageButton
        android:id="@+id/minusmax"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/betdisplay"
        app:layout_constraintRight_toLeftOf="@+id/minusmid"
        app:layout_constraintTop_toTopOf="@+id/betdisplay"
        app:layout_constraintVertical_bias="0.484"
        app:srcCompat="@drawable/minus" />

    <ImageButton
        android:id="@+id/minusmid"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/betdisplay"
        app:layout_constraintRight_toLeftOf="@+id/minusmin"
        app:layout_constraintTop_toTopOf="@+id/betdisplay"
        app:layout_constraintVertical_bias="0.484"
        app:srcCompat="@drawable/minus" />

    <ImageButton
        android:id="@+id/minusmin"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/betdisplay"
        app:layout_constraintRight_toLeftOf="@+id/betdisplay"
        app:layout_constraintTop_toTopOf="@+id/betdisplay"
        app:srcCompat="@drawable/minus"
        app:layout_constraintVertical_bias="0.484" />

    <ImageButton
        android:id="@+id/leftBtn"
        android:layout_width="101dp"
        android:layout_height="62dp"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/mainBtn"
        app:srcCompat="@drawable/play" />

    <ImageButton
        android:id="@+id/mainBtn"
        android:layout_width="111dp"
        android:layout_height="74dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/bail" />

    <ImageButton
        android:id="@+id/rightBtn"
        android:layout_width="101dp"
        android:layout_height="62dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/mainBtn"
        app:srcCompat="@drawable/play" />

    <TextView
        android:id="@+id/v1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="v"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/inventory" />

    <TextView
        android:id="@+id/v12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="0dp"
        android:text="v"
        app:layout_constraintBottom_toBottomOf="@+id/v11"
        app:layout_constraintLeft_toRightOf="@+id/v11"
        app:layout_constraintTop_toTopOf="@+id/v11" />

    <TextView
        android:id="@+id/v11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="v"
        app:layout_constraintLeft_toRightOf="@+id/v10"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/v10"
        android:layout_marginBottom="0dp"
        app:layout_constraintTop_toTopOf="@+id/v10"
        android:layout_marginTop="0dp" />

    <TextView
        android:id="@+id/v10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="v"
        app:layout_constraintBottom_toBottomOf="@+id/v9"
        android:layout_marginBottom="0dp"
        app:layout_constraintLeft_toRightOf="@+id/v9"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toTopOf="@+id/v9"
        android:layout_marginTop="0dp" />

    <TextView
        android:id="@+id/v9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="v"
        app:layout_constraintBottom_toBottomOf="@+id/v8"
        android:layout_marginBottom="0dp"
        app:layout_constraintTop_toTopOf="@+id/v8"
        android:layout_marginTop="0dp"
        app:layout_constraintLeft_toRightOf="@+id/v8"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/v8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="v"
        app:layout_constraintLeft_toRightOf="@+id/v7"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/v7"
        android:layout_marginBottom="0dp"
        app:layout_constraintTop_toTopOf="@+id/v7"
        android:layout_marginTop="0dp" />

    <TextView
        android:id="@+id/v7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="v"
        app:layout_constraintTop_toTopOf="@+id/v6"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/v6"
        android:layout_marginBottom="0dp"
        app:layout_constraintLeft_toRightOf="@+id/v6"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/v6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="v"
        app:layout_constraintLeft_toRightOf="@+id/v5"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toTopOf="@+id/v5"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/v5"
        android:layout_marginBottom="0dp" />

    <TextView
        android:id="@+id/v5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="v"
        app:layout_constraintLeft_toRightOf="@+id/v4"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toTopOf="@+id/v4"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/v4"
        android:layout_marginBottom="0dp" />

    <TextView
        android:id="@+id/v4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="v"
        app:layout_constraintLeft_toRightOf="@+id/v3"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toTopOf="@+id/v3"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/v3"
        android:layout_marginBottom="0dp" />

    <TextView
        android:id="@+id/v3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="v"
        app:layout_constraintTop_toTopOf="@+id/v2"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/v2"
        android:layout_marginBottom="0dp"
        app:layout_constraintLeft_toRightOf="@+id/v2"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/v2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="v"
        app:layout_constraintBottom_toBottomOf="@+id/v1"
        android:layout_marginBottom="0dp"
        app:layout_constraintTop_toTopOf="@+id/v1"
        android:layout_marginTop="0dp"
        app:layout_constraintLeft_toRightOf="@+id/v1"
        android:layout_marginLeft="16dp" />

    <ImageButton
        android:id="@+id/inventory"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageButton
        android:id="@+id/settings"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher_round" />


</android.support.constraint.ConstraintLayout>
